<Project>

  <PropertyGroup>
    <AnalysisMode>All</AnalysisMode>
    <VersionPrefix>1.5.1</VersionPrefix>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyOriginatorKeyFile>$(MSBuildThisFileDirectory)nein-math.snk</AssemblyOriginatorKeyFile>
    <SignAssembly>true</SignAssembly>
  </PropertyGroup>

  <PropertyGroup>
    <NoWarn>$(NoWarn);CA1014;CA1515;CA1716;CA1720;CA1825;CA5394;NU5105</NoWarn>
  </PropertyGroup>

</Project>
