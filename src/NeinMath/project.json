{
  "version": "1.0.2-*",
  "description": "NeinMath is playing around with arbitrary precision integers, written in pure managed code, not using any unsafe stuff, and a bit faster than the build-in .NET type for integers with a few thousand bits.",
  "authors": [ "Axel Heer" ],
  "copyright": "Copyright © 2011-2016 Axel Heer",

  "buildOptions": {
    "debugType": "portable",
    "keyFile": "../NeinMath.snk",
    "xmlDoc": true
  },

  "packOptions": {
    "tags": [ "Math", "BigInteger" ],
    "projectUrl": "http://github.com/axelheer/nein-math",
    "licenseUrl": "http://opensource.org/licenses/mit",
    "iconUrl": "http://home.heer.eu/media/axel.png",

    "repository": {
      "type": "git",
      "url": "http://github.com/axelheer/nein-math"
    },

    "releaseNotes": "Minor improvements."
  },

  "frameworks": {
    "net40": { },
    "netstandard1.0": {
      "dependencies": {
        "NETStandard.Library": "1.6.0"
      }
    }
  }
}
